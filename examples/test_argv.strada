# test_argv.strada - Test argc/argv parameters in main()

func main(int $argc, array $argv) int {
    say("Command-line Arguments Test");
    say("============================");
    say("");
    say("Argument count: " . $argc);
    say("");
    say("Arguments:");

    my int $i = 0;
    while ($i < $argc) {
        say("  argv[" . $i . "] = " . $argv->[$i]);
        $i = $i + 1;
    }

    return 0;
}
